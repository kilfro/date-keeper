{
  "name": "date-keeper",
  "version": "0.0.1",
  "scripts": {
    "start": "webpack serve --mode development --open 'Google Chrome'",
    "build": "webpack --mode production",
    "test": "jest",
    "test:c": "jest --coverage",
    "test:w": "jest --watch"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "prop-types": "15.7.2",
    "react": "^17.0.1",
    "react-bootstrap-icons": "^1.4.0",
    "react-dom": "^17.0.1",
    "react-outside-click-handler": "^1.3.0",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-react": "^7.12.10",
    "@testing-library/jest-dom": "^4.2.4",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.0.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-17-updated": "1.0.2",
    "html-webpack-plugin": "^4.5.0",
    "jest": "26.6.3",
    "jest-enzyme": "^7.1.2",
    "jsdom": "^16.4.0",
    "redux-mock-store": "^1.5.4",
    "style-loader": "^2.0.0",
    "webpack": "^5.10.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!**/dist/**",
      "!webpack.config.js",
      "!**/index.js"
    ]
  }
}
